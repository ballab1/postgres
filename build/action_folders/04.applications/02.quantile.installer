#!/bin/bash

#set -o verbose
set -o errexit
set -o nounset
#set -o xtrace

declare -r QUANTILE_URL="https://github.com/tvondra/quantile.git"

cd /usr/src
git clone -q "${QUANTILE_URL}"
cd /usr/src/quantile
git checkout -b "local/${QUANTILE_VERSION}" "tags/${QUANTILE_VERSION}"  
make
make install    
